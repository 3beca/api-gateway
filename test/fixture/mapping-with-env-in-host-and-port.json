{
    "version": 1,
    "headers": {
        "X-Api-Gateway": "api-gateway"
    },
    "services": [
        {
            "name": "auth",
            "protocol": "AUTH_PROTOCOL",
            "host": "AUTH_HOST",
            "port": "AUTH_PORT",
            "basePath": "/auth",
            "mappings": [
                {
                    "path": "/login",
                    "method": "GET",
                    "middlewares": []
                }
            ]
        }
    ]
}